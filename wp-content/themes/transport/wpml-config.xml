<wpml-config>
    <custom-types>
        <custom-type translate="1">faq</custom-type>
        <custom-type translate="1">location</custom-type>
        <custom-type translate="1">service</custom-type>
        <custom-type translate="1">team_member</custom-type>
        <custom-type translate="1">testimonial</custom-type>
    </custom-types>   
   
    <custom-fields>
        <custom-field action="translate">tree_head</custom-field>
        <custom-field action="translate">tree_foot</custom-field>
        <custom-field action="translate">tree_item</custom-field>
        <custom-field action="translate">transport_sub_title</custom-field>
        <custom-field action="translate">transport_banner_button_label</custom-field>
        <custom-field action="translate">transport_banner_button_link</custom-field>
        <custom-field action="translate">transport_banner_title</custom-field>
        <custom-field action="translate">transport_banner_image</custom-field>
        <custom-field action="translate">contact_form</custom-field>
        <custom-field action="translate">location_sub_title</custom-field>
        <custom-field action="translate">location_title</custom-field>
        <custom-field action="translate">member_position</custom-field>
        <custom-field action="translate">member_description</custom-field>
        <custom-field action="translate">member_social_links</custom-field>
        <custom-field action="translate">testimonial_author_position</custom-field>
        <custom-field action="translate">testimonial_author_url</custom-field>
        <custom-field action="translate">services_slider_title</custom-field>
        <custom-field action="translate">services_slider_sub_title</custom-field>
        <custom-field action="translate">services_slider_description</custom-field>
    </custom-fields>    
    <admin-texts>
        <key name="ot_option">
            <key name="transport_404_page_title" />
            <key name="transport_404_page_sub_title" />
            <key name="transport_banner_button_label" />
            <key name="coming_soon_title" />
            <key name="coming_soon_description" />
            <key name="email_label_text" />
            <key name="request_quote_label_text" />
            <key name="phone_no_label" />
            <key name="transport_query_button_label" />
            <key name="transport_query_button_link" />
            <key name="transport_query_title" />
            <key name="transport_query_description" />
            <key name="transport_copyright" />
            <key name="transport_social_icons" />
            <key name="transport_shop_banner_title" />
            <key name="transport_cart_banner_title" />
            <key name="transport_checkout_banner_title" />
            <key name="transport_account_banner_title" />
        </key> 
    </admin-texts>  
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_language_switcher_sidebar">0</key>
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>
    </language-switcher-settings>     
</wpml-config>